<ResourceDictionary xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:sys="clr-namespace:System;assembly=netstandard">
		
	<SolidColorBrush x:Key="TextBlock-Header-Normal" Color="{DynamicResource TextBlock-Header-Normal-Color}"/>
		
	<SolidColorBrush x:Key="TextBlock-Header-Emphasized" Color="{DynamicResource TextBlock-Header-Emphasized-Color}"/>
		
	<SolidColorBrush x:Key="TextBlock-Soft" Color="{DynamicResource TextBlock-Soft-Color}"/>
</ResourceDictionary>
